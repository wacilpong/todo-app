{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:55555",
  "main": "public/electron.js",
  "scripts": {
    "start:electron:dev": "concurrently \"BROWSER=none npm run start:dev\" \"wait-on http://localhost:3000 && electron .\"",
    "start:dev": "concurrently --kill-others \"npm run start:dev:server\" \"npm run start:dev:client\"",
    "start:dev:client": "react-scripts start",
    "start:dev:server": "nodemon ./backend/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "qs": "^6.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "sequelize": "^5.21.5",
    "sqlite3": "^4.1.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.3",
    "@types/qs": "^6.9.1",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^5.1.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "nodemon": "^2.0.2",
    "wait-on": "^5.0.0"
  }
}
